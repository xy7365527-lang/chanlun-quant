# TradingAgents / LLM
CLQ_TA_ENTRYPOINT=trading_agents.tradingagents.graph.trading_graph:TradingAgentsGraph
CLQ_TA_PROVIDER=deepseek
CLQ_TA_API_BASE=https://api.siliconflow.com/v1
CLQ_TA_API_KEY=__PUT_YOUR_KEY_HERE__
CLQ_TA_MODEL=deepseek-ai/DeepSeek-V3.2-Exp

# Market data factory
CLQ_MKD_FACTORY=chanlun_quant.integration.market_data:make_market_datas

# Candidate aggregator & fundamentals provider
CLQ_CANDIDATE_RUNNER=chanlun_quant.selectors.candidate_aggregator:merge_candidates
CLQ_FUNDA_PROVIDER=chanlun_quant.integration.fundamentals:get_fundamentals

# Universe / frequencies / lookback
CLQ_CODES=AAPL,MSFT,NVDA
CLQ_FREQUENCYS=d,60m
CLQ_LOOKBACK=800

# Selector defaults
CLQ_FREQ=d
CLQ_MAX_CANDIDATES=80
CLQ_TOP_K=2
CLQ_MIN_SCORE=0.0

# Logging
CLQ_LOG_LEVEL=INFO
